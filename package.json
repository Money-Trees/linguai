{
  "name": "@linguai/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "checkEnv": "node ./tools/checkEnv.js",
    "start": "npm run checkEnv && nx serve",
    "start:prod": "node dist/apps/backend/main.js",
    "build": "nx run-many --target=build --projects=frontend,backend --parallel=true",
    "test": "nx run-many --target=test --all=true",
    "dev": "npm run checkEnv && nx run-many --target=serve --all=true",
    "lint": "nx run-many --target=lint --all=true",
    "gen:theme-typings": "chakra-cli tokens apps/frontend/src/theme.ts",
    "postinstall": "npm run gen:theme-typings",
    "prepare": "husky install",
    "migrations:generate": "nx run backend:typeorm-generate-migrations",
    "migrations:run": "nx run backend:typeorm-run-migrations",
    "migrations:revert": "nx run backend:typeorm-revert-migrations",
    "db": "docker-compose up",
    "db:wipe": "docker-compose down --volumes",
    "db:reset": "npm run db:wipe && npm run db",
    "db:seed": "chmod +x ./tools/database/seed.sh; ./tools/database/seed.sh"
  },
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^2.7.1",
    "@choc-ui/chakra-autocomplete": "^5.1.5",
    "@nestjs/common": "10.0.5",
