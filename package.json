    "@babel/preset-react": "^7.14.5",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.5",
    "@nx/cypress": "16.5.1",
    "@nx/eslint-plugin": "16.5.1",
    "@nx/jest": "16.5.1",
    "@nx/js": "16.5.1",
    "@nx/linter": "16.5.1",
    "@nx/nest": "16.5.1",
    "@nx/node": "16.5.1",
    "@nx/react": "16.5.1",
    "@nx/webpack": "16.5.1",
    "@nx/workspace": "16.5.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^8.0.1",
    "@swc/cli": "~0.1.62",
    "@swc/core": "~1.3.51",
    "@testing-library/react": "14.0.0",
    "@types/express": "^4.17.17",
